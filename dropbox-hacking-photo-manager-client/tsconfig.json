{
  "compilerOptions": {
    "baseUrl": "src",
    "jsx": "react",
    "lib": ["ES2024", "DOM", "DOM.Iterable"],
    "module": "Preserve",
    "moduleResolution": "bundler",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "outDir": "./public/dist/",
    "paths": {
      "@/*": ["./*"]
    },
    "resolveJsonModule": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es6"
  },
  "references": [
    {
      "path": "../dropbox-hacking-photo-manager-shared"
    }
  ]
}
