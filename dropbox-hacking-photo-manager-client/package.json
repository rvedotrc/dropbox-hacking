    "@eslint/js": "^9.21.0",
    "@testing-library/react": "^16.1.0",
    "@types/events": "^3.0.3",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "@types/react-modal": "^3.10.5",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
