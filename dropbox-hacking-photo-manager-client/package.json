{
  "author": "Rachel Evans <rachel@rachelevans.org>",
  "dependencies": {
    "array-uniq": "^3.0.0",
    "dropbox-hacking-photo-manager-shared": "link:../dropbox-hacking-photo-manager-shared",
    "events": "^3.3.0",
