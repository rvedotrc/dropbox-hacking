    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.25.0"
  },
  "directories": {
    "lib": "lib"
  },
  "license": "MIT",
  "main": "index.js",
  "name": "dropbox-hacking-photo-manager-client",
  "scripts": {
    "build": "pnpm format && pnpm run test:lint && pnpm run compile",
    "build:watch": "rebuild -r src -- sh -c 'if ! pnpm test:prettier ; then pnpm format ; else pnpm run test:lint && pnpm run compile ; fi'",
    "compile": "webpack --mode development",
    "format": "prettier --write .",
    "rebuild": "rebuild package.json package-lock.json tsconfig.json .babelrc webpack.config.js src .git/refs -- npm run-script build",
    "test": "concurrently --group pnpm:test:*",
    "test:prettier": "prettier --check .",
    "test:lint": "eslint",
    "test:unit": "c8 node --enable-source-maps --experimental-test-coverage --test './dist/spec/**/*.js'"
  },
  "version": "1.0.0"
}
